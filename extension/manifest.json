{
  "manifest_version": 3,
  "name": "Google Chat - エクスプローラーで開く",
  "version": "1.0.0",
  "description": "Google Chatで選択したパスを右クリックからエクスプローラーで開きます",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "contextMenus",
    "nativeMessaging",
    "activeTab",
    "scripting",
    "notifications"
  ],
  "host_permissions": [
    "https://chat.google.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
